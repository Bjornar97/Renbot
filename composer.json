{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2.0",
        "ghostzero/tmi": "^2.3.3",
        "guzzlehttp/guzzle": "^7.8.1",
        "inertiajs/inertia-laravel": "^v0.6.11",
        "laravel/framework": "^v10.39.0",
        "laravel/pennant": "^v1.5.1",
        "laravel/sanctum": "^v3.3.3",
        "laravel/socialite": "^v5.11.0",
        "laravel/telescope": "^v4.17.3",
        "laravel/tinker": "^v2.8.2",
        "pusher/pusher-php-server": "^7.2.4",
        "romanzipp/laravel-twitch": "^4.6.4",
        "socialiteproviders/twitch": "^5.3.1",
        "spatie/laravel-activitylog": "^4.7.3",
        "spatie/laravel-webhook-client": "^3.2.0",
        "tightenco/ziggy": "^v1.8.1"
    },
    "require-dev": {
        "fakerphp/faker": "^v1.23.1",
        "laravel/pint": "^v1.13.7",
        "laravel/sail": "^v1.26.3",
        "mockery/mockery": "^1.6.7",
        "nunomaduro/collision": "v7.10.0",
        "pestphp/pest": "v2.30.0",
        "pestphp/pest-plugin-laravel": "^v2.2.0",
        "phpunit/phpunit": "^10.5.5",
        "spatie/laravel-ignition": "^2.4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
